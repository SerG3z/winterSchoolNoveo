<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MyTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:colorPrimary">#009688</item>
        <item name="android:colorPrimaryDark">#00796B</item>
        <item name="android:colorAccent">#B2DFDB</item>
        <item name="android:textColorPrimary">#000</item>
    </style>

    <style name="MyTheme.Popup" parent="Base.ThemeOverlay.AppCompat">
        <item name="android:textColorPrimary">#000</item>
        <item name="android:background">#00796B</item>
    </style>

    <style name="MyThemeSpinner" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:colorPrimaryDark">#F57F17</item>
        <item name="android:colorPrimary">#F9A825</item>
        <item name="android:colorAccent">#FFEB3B</item>
        <item name="android:textColorPrimary">#FFFDE7</item>
    </style>

    <style name="MyThemeSpinner.Popup" parent="Base.ThemeOverlay.AppCompat">
        <item name="android:textColorPrimary">#FFFDE7</item>
        <item name="android:background">#F57F17</item>
    </style>



</resources>